{
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "background": {
        "scripts": ["js/mono.js","js/background.js"]
    },
    "content_scripts": [
        {
          "matches": ["*://ya.ru/*"],
          "js": ["/js/mono.js", "/js/inject.js"]
        }
    ],
    "permissions": [
        "tabs"
    ],
    "name": "Mono test!",
    "version": "0.1"
}